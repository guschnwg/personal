{
  "private": true,
  "name": "front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --no-source-maps --out-dir build --public-url /front/ --no-cache --hmr-port 37735",
    "dev": "parcel watch src/index.html --no-source-maps --out-dir build --public-url /front/ --no-cache --hmr-port 37735",
    "build": "parcel build src/index.html --no-source-maps --out-dir build --public-url /front/ --no-cache",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0"
  },
  "dependencies": {
    "events": "^3.3.0",
    "pixi.js": "^6.0.4",
    "pixi.js-keyboard": "^1.1.6",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "src/assets/game",
        "staticOutDir": "assets/game"
      },
      {
        "staticPath": "src/assets/videos",
        "staticOutDir": "assets/videos"
      }
    ]
  }
}
